<templates xml:space="preserve">
    <t t-name="account_reports.redirectAction">
        <Dialog size="'md'">
            <div role="alert">
                <p class="text-prewrap">It seems there is some depending closing move to be posted</p>
            </div>
            <t t-set-slot="footer">
                <button class="btn btn-primary" t-on-click="openClientAction">Depending moves</button>
                <button class="btn btn-secondary" t-on-click="close">Cancel</button>
            </t>
        </Dialog>
    </t>
</templates>
