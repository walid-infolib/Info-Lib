<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payment_provider_flouci" model="payment.provider">
        <field name="name">Flouci</field>
        <field name="code">flouci</field>
        <field name="image_128"
               type="base64"
               file="payment_flouci/static/description/icon.png"
        />
        <field name="module_id" ref="base.module_payment_flouci"/>
        <field name="redirect_form_view_id" ref="redirect_form" />
        <field name="payment_method_ids"
               eval="[(6, 0, [
                   ref('payment_flouci.payment_method_flouci_methods')
               ])]"
        />
        <field name="available_currency_ids" eval="[(6, 0, [
                ref('base.TND')
            ])]"
        />
        <field name="available_country_ids" eval="[(6, 0, [
                ref('base.tn')
            ])]"
        />
    </record>

</odoo>