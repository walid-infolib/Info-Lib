<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payment_method_flouci_methods" model="payment.method">
        <field name="name">Flouci Methods</field>
        <field name="code">flouci_methods</field>
        <field name="sequence">1</field>
        <field name="active">True</field>
        <field name="image" type="base64" file="payment_flouci/static/img/flouci_methods.png"/>
        <field name="support_tokenization">False</field>
        <field name="support_express_checkout">False</field>
        <field name="support_refund">none</field>
        <field name="supported_currency_ids"
               eval="[Command.set([
                         ref('base.TND'),
                     ])]"
        />
        <field name="supported_country_ids"
               eval="[Command.set([
                         ref('base.tn'),
                     ])]"
        />
    </record>

</odoo>
